{
  "name": "plainwind",
  "displayName": "Plainwind",
  "description": "Tailwind to Plain English Translator",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.80.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onLanguage:javascript",
    "onLanguage:javascriptreact",
